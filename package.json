{
  "name": "@zcong/koa-jaeger-tracing",
  "version": "0.2.2",
  "description": "out of box jaeger tracing helper package for koa",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run lint && npm run jest",
    "test:cov": "npm run lint && npm run jest:cov",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "jest": "jest --forceExit",
    "jest:cov": "jest --coverage --forceExit",
    "prepublishOnly": "npm run build",
    "release": "shipjs prepare"
  },
  "types": "./dist/index.d.ts",
  "author": "zcong1993",
  "license": "MIT",
  "devDependencies": {
    "@types/jaeger-client": "^3.15.3",
    "@types/jest": "^26.0.0",
    "@types/koa": "^2.11.2",
    "@types/koa-router": "^7.4.0",
    "@types/node": "^11.9.4",
    "@types/supertest": "^2.0.8",
    "husky": "^4.0.0",
    "jest": "^26.0.0",
    "koa": "^2.11.0",
    "koa-router": "^9.0.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.0",
    "shipjs": "0.21.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "*.{js,json,ts}": [
      "yarn format",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && lint-staged"
    }
  },
  "dependencies": {
    "asyncctx": "^2.0.11",
    "axios": "^0.20.0",
    "jaeger-client": "^3.17.2",
    "opentracing": "^0.14.4"
  },
  "peerDependencies": {
    "koa": "*"
  }
}
